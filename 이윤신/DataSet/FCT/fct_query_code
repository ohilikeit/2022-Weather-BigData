fct_d_D02_2015_area_fin <- merge(fct_d_D02_2015_area,fct_code, by = "imi_code")
fct_d_D02_2016_area_fin <- merge(fct_d_D02_2016_area,fct_code, by = "imi_code")

write.csv(fct_d_D02_2012_area_fin, 'fct_d_D02_2012_area_fin.csv', fileEncoding = 'cp949')
