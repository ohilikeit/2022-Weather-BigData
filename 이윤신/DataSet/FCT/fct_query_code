fct_e_A11_2012_area <-  merge(fct_e_A11_2012, gisang_zizum, by = "stn_id")
fct_e_A11_2013_area <-  merge(fct_e_A11_2013, gisang_zizum, by = "stn_id")
fct_e_A11_2014_area <-  merge(fct_e_A11_2014, gisang_zizum, by = "stn_id")
fct_e_A11_2015_area <-  merge(fct_e_A11_2015, gisang_zizum, by = "stn_id")
fct_e_A11_2016_area <-  merge(fct_e_A11_2016, gisang_zizum, by = "stn_id")

fct_e_A11_2012_area_fin <- merge(fct_e_A11_2012_area,fct_code, by = "imi_code")
fct_e_A11_2013_area_fin <- merge(fct_e_A11_2013_area,fct_code, by = "imi_code")
fct_e_A11_2014_area_fin <- merge(fct_e_A11_2014_area,fct_code, by = "imi_code")
fct_e_A11_2015_area_fin <- merge(fct_e_A11_2015_area,fct_code, by = "imi_code")
fct_e_A11_2016_area_fin <- merge(fct_e_A11_2016_area,fct_code, by = "imi_code")

write.csv(fct_e_A11_2012_area_fin, 'fct_e_A11_2012_area_fin.csv', fileEncoding = 'cp949')
write.csv(fct_e_A11_2013_area_fin, 'fct_e_A11_2013_area_fin.csv', fileEncoding = 'cp949')
write.csv(fct_e_A11_2014_area_fin, 'fct_e_A11_2014_area_fin.csv', fileEncoding = 'cp949')
write.csv(fct_e_A11_2015_area_fin, 'fct_e_A11_2015_area_fin.csv', fileEncoding = 'cp949')
write.csv(fct_e_A11_2016_area_fin, 'fct_e_A11_2016_area_fin.csv', fileEncoding = 'cp949')

















fct_e_A12_2012<- dbGetQuery(conn, "SELECT TM_FC, IMI_CODE, STN_ID,T1,T2,T3,T4,T5,T6,T7,T8,T9,T10,T11,T12,T13,T14,T15
			FROM FCT_IMI2_LIFE2
			WHERE  STN_ID < 500
				AND IMI_CODE like 'A12'
				AND TM_FC like '2012%'")
fct_e_A12_2013<- dbGetQuery(conn, "SELECT TM_FC, IMI_CODE, STN_ID,T1,T2,T3,T4,T5,T6,T7,T8,T9,T10,T11,T12,T13,T14,T15
			FROM FCT_IMI2_LIFE2
			WHERE  STN_ID < 500
				AND IMI_CODE like 'A12'
				AND TM_FC like '2013%'")
fct_e_A12_2014<- dbGetQuery(conn, "SELECT TM_FC, IMI_CODE, STN_ID,T1,T2,T3,T4,T5,T6,T7,T8,T9,T10,T11,T12,T13,T14,T15
			FROM FCT_IMI2_LIFE2
			WHERE  STN_ID < 500
				AND IMI_CODE like 'A12'
				AND TM_FC like '2014%'")
fct_e_A12_2015<- dbGetQuery(conn, "SELECT TM_FC, IMI_CODE, STN_ID,T1,T2,T3,T4,T5,T6,T7,T8,T9,T10,T11,T12,T13,T14,T15
			FROM FCT_IMI2_LIFE2
			WHERE  STN_ID < 500
				AND IMI_CODE like 'A12'
				AND TM_FC like '2015%'")
fct_e_A12_2016<- dbGetQuery(conn, "SELECT TM_FC, IMI_CODE, STN_ID,T1,T2,T3,T4,T5,T6,T7,T8,T9,T10,T11,T12,T13,T14,T15
			FROM FCT_IMI2_LIFE2
			WHERE  STN_ID < 500
				AND IMI_CODE like 'A12'
				AND TM_FC like '2016%'")
fct_e_A12_2012_area <-  merge(fct_e_A12_2012, gisang_zizum, by = "stn_id")
fct_e_A12_2013_area <-  merge(fct_e_A12_2013, gisang_zizum, by = "stn_id")
fct_e_A12_2014_area <-  merge(fct_e_A12_2014, gisang_zizum, by = "stn_id")
fct_e_A12_2015_area <-  merge(fct_e_A12_2015, gisang_zizum, by = "stn_id")
fct_e_A12_2016_area <-  merge(fct_e_A12_2016, gisang_zizum, by = "stn_id")

fct_e_A12_2012_area_fin <- merge(fct_e_A12_2012_area,fct_code, by = "imi_code")
fct_e_A12_2013_area_fin <- merge(fct_e_A12_2013_area,fct_code, by = "imi_code")
fct_e_A12_2014_area_fin <- merge(fct_e_A12_2014_area,fct_code, by = "imi_code")
fct_e_A12_2015_area_fin <- merge(fct_e_A12_2015_area,fct_code, by = "imi_code")
fct_e_A12_2016_area_fin <- merge(fct_e_A12_2016_area,fct_code, by = "imi_code")

write.csv(fct_e_A12_2012_area_fin, 'fct_e_A12_2012_area_fin.csv', fileEncoding = 'cp949')
